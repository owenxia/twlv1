<%= simple_form_for @experience do |f| %>
	<%= f.input :name %>
	<%= f.input :continent, collection: ['Asia', 'Europe', 'North America', 'South America', 'Oceania', 'Africa', 'Antarctica'], prompt: "Select"  %>
	<%= f.input :country %>
	<%= f.input :city %>
	<%= f.input :location %>
	<%= f.input :tag_list, label: 'Keywords' %>
	<%= f.input :cover, as: :file %>
	<%= f.input :description %>
	<%= f.input :multimedia %>
	<%= f.input :external_links %>
	<%= f.button :submit, class: "btn btn-primary" %>
<% end %>
